{
  "name": "covid-19-tracker-electron",
  "productName": "Covid-19 Tracker",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Thanh Vu <misa198>",
  "private": true,
  "main": "electron/main.js",
  "homepage": "./",
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "build/icon.icns"
    },
    "linux": {
      "target": [
        "deb"
      ],
      "icon": "assets/icon.icns"
    }
  },
  "scripts": {
    "react:start": "export BROWSER=none && react-scripts start",
    "react:build": "react-scripts build",
    "electron:start": "wait-on tcp:3000 && electron .",
    "electron:build": "mkdir build/src && cp -r electron/. build/electron",
    "clean": "rm -rf build dist",
    "package": "electron-builder build --mac --linux -c.extraMetadata.main=build/electron/main.js --publish never",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "yarn clean && yarn react:build && yarn electron:build && yarn package",
    "dev": "concurrently -k \"yarn react:start\" \"yarn electron:start\""
  },
  "dependencies": {
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@fontsource/roboto": "^4.5.1",
    "@mui/icons-material": "^5.2.0",
    "@mui/material": "^5.2.1",
    "@mui/styles": "^5.2.1",
    "@mui/utils": "^5.2.1",
    "@react-hook/window-size": "^3.0.7",
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "apexcharts": "^3.30.0",
    "axios": "^0.24.0",
    "axios-cache-adapter": "^2.7.3",
    "body-scroll-lock": "^4.0.0-beta.0",
    "dayjs": "^1.10.7",
    "electron-is-dev": "^2.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-detect-offline": "^2.4.5",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "slugify": "^1.6.3",
    "web-vitals": "^1.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.4.0",
    "electron": "^16.0.2",
    "electron-builder": "^22.14.5",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "19.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.0",
    "wait-on": "^6.0.0"
  }
}
